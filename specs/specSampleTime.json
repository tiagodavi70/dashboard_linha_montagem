{
    "config": {
      "view": {"continuousWidth": 400, "continuousHeight": 300},
      "axis": {"labelColor": "white", "titleColor": "white"},
      "background": "#122973",
      "header": {
        "labelColor": "#122973",
        "labelFontSize": 14,
        "titleColor": "#122973",
        "titleFontSize": 14
      }
    },
    "layer": [
      {
        "data": {
          "url": "https://raw.githubusercontent.com/tiagodavi70/dashboard_linha_montagem/main/simulation/CycleTimes.json"
        },
        "mark": {"type": "line", "color": "gold"},
        "encoding": {
          "x": {"field": "timeStamp", "type": "temporal"},
          "y": {"field": "totalCycleTime", "type": "quantitative"}
        },
        "transform": [{"filter": "datum.station == '260'"}]
      },
      {
        "data": {
          "url": "https://raw.githubusercontent.com/tiagodavi70/dashboard_linha_montagem/main/simulation/CycleTimes.json"
        },
        "mark": {"type": "rule", "color": "red"},
        "encoding": {
          "size": {"value": 6},
          "y": {
            "aggregate": "mean",
            "field": "totalCycleTime",
            "type": "quantitative"
          }
        },
        "transform": [{"filter": "datum.station == '260'"}]
      }
    ],
    "transform": [{"filter": "datum.index >= 0 && datum.index < 100"}],
    "width": 600,
    "data": {
      "url": "https://raw.githubusercontent.com/tiagodavi70/dashboard_linha_montagem/main/simulation/CycleTimes.json"
    },
    "$schema": "https://vega.github.io/schema/vega-lite/v4.17.0.json"
  }