{
  "config": {
    "view": {"continuousWidth": 400, "continuousHeight": 300},
    "axis": {"labelColor": "white", "titleColor": "white"},
    "background": "#122973",
    "header": {
      "labelColor": "#122973",
      "labelFontSize": 14,
      "titleColor": "#122973",
      "titleFontSize": 14
    }
  },
  "layer": [
    {
      "data": {
        "url": "https://raw.githubusercontent.com/tiagodavi70/dashboard_linha_montagem/main/simulation/KPI.json"
      },
      "mark": {"type": "line", "color": "gold"},
      "encoding": {
        "x": {"field": "index", "type": "quantitative"},
        "y": {"field": "oee", "type": "quantitative"}
      },
      "transform": [{"filter": "datum.station == '210'"}]
    },
    {
      "data": {
        "url": "https://raw.githubusercontent.com/tiagodavi70/dashboard_linha_montagem/main/simulation/KPI.json"
      },
      "mark": {"type": "rule", "color": "red"},
      "encoding": {
        "size": {"value": 6},
        "y": {"aggregate": "mean", "field": "oee", "type": "quantitative"}
      },
      "transform": [{"filter": "datum.station == '210'"}]
    }
  ],
  "transform": [{"filter": "datum.index >= 0 && datum.index < 100"}],
  "width": 600,
  "$schema": "https://vega.github.io/schema/vega-lite/v4.17.0.json"
}